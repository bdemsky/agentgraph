#/bin/bash

mkdir -p examples/results/

for i in examples.files.example examples.python.example examples.muttest.example examples.varsettest.example examples.vardicttest.example examples.retmut.example
do
echo ==========================================================
echo $i
echo ----------------------------------------------------------
if test -e examples/results/$i.result
then
python -m $i > examples/results/$i.txt
if diff -q examples/results/$i.txt examples/results/$i.result > /dev/null
then
echo "Test passed!"
else
echo "Test failed!"
fi
else
echo "Missing result!"
python -m $i | tee examples/results/$i.txt
fi

done